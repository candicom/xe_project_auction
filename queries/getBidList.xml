<query id="getBidList" action="select">
    <tables>
        <table name="pa_bids" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="document_srl" var="document_srl" filter="number" notnull="notnull" />
    </conditions>
    <navigation>
        <index var="sort_index" default="list_order" order="asc" />
    </navigation>
</query>
